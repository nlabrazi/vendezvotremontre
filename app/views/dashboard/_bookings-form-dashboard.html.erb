<div class="my-booking">

  <h1>Liste de mes RDV</h1>

  <%= month_calendar events: @bookings do |date, bookings| %>
  <%= date %>

  <% bookings.each do |booking| %>
  <div class="result-booking">
    <p>
      A COMPLETER
      RDV#: <%= booking.id %>
      WATCH#: <%= booking.watch_id %>
    </p>
  </div>
  <% end %>
  <% end %>

</div>
